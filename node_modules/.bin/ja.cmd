@ECHO off
GOTO start
:find_dp0
SET dp0=%~dp0
EXIT /b
:start
SETLOCAL
CALL :find_dp0

IF EXIST "%dp0%\-u.exe" (
  SET "_prog=%dp0%\-u.exe"
) ELSE (
  SET "_prog=-u"
  SET PATHEXT=%PATHEXT:;.JS;=;%
)

endLocal & goto #_undefined_# 2>NUL || title %COMSPEC% & "%_prog%" DEBUG node "%dp0%\..\ja\cli\ja.js" %*
